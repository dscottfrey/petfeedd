[general]
discovery_enabled=1
database=petfeedd.db
name=Living Room Feeder

[logging]
enabled=1
; Valid values are stdout, file, or syslog.
method=stdout
; Required with the file method
file=
; Required with the syslog method.
address=

[web]
web_enabled=1
bind_address=127.0.0.1
bind_port=8080

[gpio]
servo_pin=17
servo_feed_time=0.26

[email_notifications]
