to=you@example.com
server=example.com
port=25
username=you@example.com
password=password

[pushbullet_notifications]
enabled=1
subject=Feed Event
api_key=api.key

[twitter_notifications]
enabled=0
consumer_key=
consumer_secret=
access_key=
access_secret=
